<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="SavedCreatures">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Creature" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="left" type="xs:string"/>
							<xs:element name="right" type="xs:string"/>
							<xs:element name="bodyParts">
								<xs:complexType>
									<xs:sequence>
										<xs:element maxOccurs="unbounded" name="item">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="key" type="xs:string"/>
													<xs:element name="value" type="xs:string"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>

								<xs:unique name="uniqueItem">
									<xs:selector xpath="item"/>
									<xs:field xpath="key"/>
								</xs:unique>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	

	
	
</xs:schema>
